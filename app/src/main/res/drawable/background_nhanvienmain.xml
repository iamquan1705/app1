<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="100dp" android:height="100dp">
        <shape android:shape="rectangle">
            <solid android:color="#DAAAAA"/>
            <stroke
                android:width="5dp"
                android:color="#335B1180" />
            <corners
                android:radius="15dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/people"
        android:top="20dp"
        android:bottom="20dp"
        android:left="25dp"
        android:right="20dp"
        android:width="50dp"
        android:height="50dp"
        >
    </item>

</layer-list>