        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_edit_24"
        android:top="15dp"
        android:left="18dp"
        android:right="20dp"
        android:width="15dp"
        android:height="15dp"
        >
    </item>

</layer-list>